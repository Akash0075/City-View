void cloud ()
{
      glPushMatrix();
      glTranslatef(position, 0,0);
      glColor3ub(255,255,255);

      Circle(0.83, 0.8, 0.07, 1000);
      Circle(0.75, 0.8, 0.09, 1000);
      Circle(0.68, 0.8, 0.07, 1000);


      Circle(0.52, 0.9, 0.07, 1000);
      Circle(0.6, 0.9, 0.09, 1000);
      Circle(0.68, 0.9, 0.07, 1000);


      Circle(-0.83, 0.8, 0.07, 1000);
      Circle(-0.75, 0.8, 0.09, 1000);
      Circle(-0.68, 0.8, 0.07, 1000);


      Circle(-0.52, 0.9, 0.07, 1000);
      Circle(-0.6,  0.9, 0.09, 1000);
      Circle(-0.68, 0.9, 0.07, 1000);

      Circle(-0.02, 0.9, 0.08, 1000);
      Circle(-0.15, 0.9, 0.093, 1000);
      Circle(-0.27, 0.9, 0.08, 1000);

      glPopMatrix();
}

void hills ()
{
     glColor3ub(26, 255, 26);
     Circle(0.2, 0.2, 0.3, 1000);
     Circle(0.6, -0.15, 0.5, 1000);
     Circle(0.9, 0.3, 0.2, 1000);
}

void display()
{
       glClearColor(0.3f, 0.5f, 1.0f, 1.0f);
       glClear(GL_COLOR_BUFFER_BIT);
       hills ();
       cloud ();
       Sun();
       Buildings ();
       Garden();
       River();
       Road();
       Car2();
       car();
       gate();
       Bridge();
       Boat();
       glFlush();
}
void handleMouse(int button, int state, int x, int y)

{
      if (button == GLUT_LEFT_BUTTON)
           {
               speed += 0.001f;
           }
      if (button == GLUT_RIGHT_BUTTON)
           {
               speed = -0.001f;
           }
      glutPostRedisplay();
}

void handleKeypress(unsigned char key, int x, int y)

{
     switch (key)
    {
         case 's':
             speed = 0.0f;
             break;

         case 'g':
             speed = 0.008f;
             break;
             glutPostRedisplay();
    }
}

int main(int argc, char** argv)
{
   glutInit(&argc, argv);
   glutInitWindowSize(1500,900);
   glutInitWindowPosition(50, 50);
   glutCreateWindow("move");
   glutDisplayFunc(display);
   glutTimerFunc(100, update, 0);
   glutKeyboardFunc(handleKeypress);
   glutMouseFunc(handleMouse);
   glutMainLoop();
   return 0;
}

